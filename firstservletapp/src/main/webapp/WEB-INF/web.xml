<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>firstservletapp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>myPage</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>FirstGenericServlet</servlet-name>
  	<servlet-class>firstservletapp.FirstGenericServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FirstGenericServlet</servlet-name>
  	<url-pattern>/myGenePage</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>FirstHttpServlet</servlet-name>
  	<servlet-class>firstservletapp.FirstHttpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FirstHttpServlet</servlet-name>
  	<url-pattern>/myHttpPage</url-pattern>
  	
  </servlet-mapping>
</web-app>